{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "AgatePris.Intar/AgatePris.Intar.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "gists/**",
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "Intar",
      "_appTitle": "Intar",
      "_enableSearch": true,
      "pdf": false
    },
    "markdownEngineProperties": {
      "markdigExtensions": [
        "Footnotes"
      ]
    }
  }
}